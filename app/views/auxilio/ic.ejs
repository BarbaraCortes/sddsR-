<!-- Bolsas de IC -->
<div class = "bolsas" id = "ic">
	<h2> Iniciação Científica </h2>
	<% for(var i = 0; i < bolsas_ic.length; ++i) { %>
		<hr style = "border-color: #888888">
		<div class = "post">
			<h3> <%- bolsas_ic[i].titulo %> </h3>
			<p> <%- bolsas_ic[i].desc %> </p>
			<%if(bolsas_ic[i].bullets.length > 0) {%>
				<br>
				<lu>
					<% for(var j = 0; j < bolsas_ic[i].bullets.length; ++j) {%>
						<li> <%- bolsas_ic[i].bullets[j] %> </li>
					<%}%>
				</lu>
			<%}%>
			<br><br>
			<i style="font-size: 12pt"> Mais informações: <a href="google.com"> <%-bolsas_ic[i].link%> </a> </i> <br>
			<i style="font-size: 12pt"> Última atualização: <%-bolsas_ic[i].atualizado%>  </i>
		</div>
	<%}%>
	<br>
</div>